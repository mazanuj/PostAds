        <TextBox Name="M" TabIndex="1" Height="23" VerticalAlignment="Top" TextWrapping="Wrap" Margin="208,60,0,0" HorizontalAlignment="Left" Width="37"/>
        <TextBlock x:Name="TextBlockU" Text="U" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="306,39,0,0"/>
        <TextBox Name="U" TabIndex="3" Height="23" VerticalAlignment="Top" TextWrapping="Wrap" Margin="292,60,0,0" HorizontalAlignment="Left" Width="37"/>
        <TextBlock x:Name="TextBlockP" Text="P" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="263,39,0,0"/>
        <TextBox Name="P" TabIndex="2" Height="23" VerticalAlignment="Top" TextWrapping="Wrap" Margin="250,60,0,0" HorizontalAlignment="Left" Width="37"/>
    </Grid>
</UserControl>
