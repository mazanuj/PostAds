        <Button x:Name="Save" Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="37,105,0,0" TabIndex="4"/>
        <Button x:Name="Cancel" Content="Отменить" VerticalAlignment="Top" Margin="170,105,0,0" IsCancel="True" HorizontalAlignment="Left" Width="75" TabIndex="5"/>
        <TextBlock Text="M" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="220,39,0,0" RenderTransformOrigin="7.609,0.501"/>
