        <TextBlock Text="Motosale" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="220,39,0,0" RenderTransformOrigin="7.609,0.501"/>
        <TextBlock x:Name="TextBlockName" Text="Модель" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="102,39,0,0" RenderTransformOrigin="3.326,0.741"/>
        <TextBox Name="Name" TabIndex="0" Height="23" HorizontalAlignment="Left" VerticalAlignment="Top" TextWrapping="Wrap" Width="165" Margin="37,60,0,0"/>
        <TextBox Name="Val" TabIndex="1" Height="23" VerticalAlignment="Top" TextWrapping="Wrap" Margin="208,60,0,0" HorizontalAlignment="Left" Width="37"/>
       
    </Grid>
</UserControl>
